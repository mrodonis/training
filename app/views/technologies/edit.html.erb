<h1>Editing technology</h1>

<% form_for(@technology) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :tech %><br />
    <%= f.text_field :tech %>
  </p>
  <p>
    <%= f.label :category %><br />
    <%= f.text_field :category %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @technology %> |
<%= link_to 'Back', technologies_path %>