<h1>New technology</h1>

<% form_for(@technology) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :tech %><br />
    <%= f.text_field :tech %>
  </p>
  <p>
    <%= f.label :version %><br />
    <%= f.text_field :version %>
  </p>
  <p>
    <%= f.label :category %><br />
    <%= f.text_field :category %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', technologies_path %>